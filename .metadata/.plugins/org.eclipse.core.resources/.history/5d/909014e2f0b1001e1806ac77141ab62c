	@Test
	public void LaunchBrowser(String url)
	{
		WebDriverManager.chromiumdriver().setup();
		WebDriver driver = new ChromeDriver();
		driver.get("https://wikipedia.org/");
//		return driver;
		driver.close();
	}	
}
